  <data name="customSqlQuery2.Sql" xml:space="preserve">
    <value>SELECT
			A.ID_CONOZCA,
			A.ID_CLIENTE,
			B.NOMBRES,
			B.APELLIDO_PAT,
			B.APELLIDO_MAT,
			B.CELULAR,
			B.DIRECCION_RESIDENCIAL,
			B.EMAIL,
			CASE B.ES_PEP
			WHEN 1 THEN 'SI'
			WHEN 0 THEN 'NO'
			END ES_PEP,
			B.ESTADO,
			B.ESTADO_CIVIL,
			B.FECHA_NACIMIENTO,
			B.ID_LUGAR_NACIMIENTO,
			B.LUGAR_NACIMIENTO,
			B.ID_NACIONALIDAD,
			B.NACIONALIDAD,
			B.ID_OCUPACION,
			B.OCUPACION,
			B.ID_OFICINA,
			B.OFICINA,
			B.ID_PROVINCIA,
			B.PROVINCIA,
			B.ID_TIPO_IDENTIFICACION,
			B.DESCRIPCION TIPO_IDENTIFICACION,
			B.IDENTIFICACION,
			B.SEXO,
			B.TELEFONO,
			B.TIPO_CLIENTE,
			B.TIPO_VIVIENDA,
			CASE B.VINCULADO_PEP
			WHEN 1 THEN 'SI'
			WHEN 0 THEN 'NO'
			END VINCULADO_PEP,
			A.ACTIVIDAD_CAGO_PEP,
			A.ACTIVIDAD_DEL_NEGOCIO,
			A.ACTIVIDAD_EMPRESA,
			A.ANTIGUEDAD_CONYUGUE,
			A.CARGO_CONYUGUE,
			A.CARGO_EN_EMPRESA,
			A.DIRECCION_EMPRESA,
			A.DIRECCION_NEGOCIO,
			A.DIRECCION_TRABAJO_CONYUGUE,
			A.EMPRESA_TRAJAJO_CONYUGUE,
			A.EMPRESA_DONDE_LABORA,
			A.EMPRESA_TRAJAJO_CONYUGUE,
			A.FECHA,
			A.FECHA_INGRESO,
			A.NACIONALIDAD_CONYUGUE,
			A.IDENTIFICACION_BENEFICIARIO,
			A.IDENTIFICACION_CONYUGUE,
			A.INGRESO_MENSUAL,
		    CASE A.NEGOCIO_PROPIO
			WHEN 1 THEN 'SI'
			WHEN 0 THEN 'NO'
			END NEGOCIO_PROPIO,
			A.NOMBRE_BENEFICIARIO,
			A.NOMBRE_CONYUGUE,
			A.NOMBRE_NEGOCIO,
			A.NOMBRE_PEP1,
			A.NOMBRE_PEP2,
			A.NUMERO_RUC,
			A.OCUPACION_CONYUGUE,
			A.PARENTEZCO1,
			A.PARENTEZCO2,
			A.REFERENCIA_UBICACION_EMPRESA,
			A.REFERENCIA_UBICACION_NEGOCIO,
			A.SUELDO,
			A.SUELDO_MENSUAL_CONYUGUE,
			A.TELEFONO_EMPRESA,
			A.TELEFONO_NEGOCIO,
			A.TELEFONO_TRABAJO_CONYUGUE,
			A.TIEMPO_EN_EL_MERCADO,
            A.COMO_SE_ENTERO,
			isnull(C.VALOR_TOTAL,0) VALOR_RIESGO
			FROM CONOZCA_SU_CLIENTE A, V_CLIENTES B, EVALUACIONES C
			WHERE A.ID_CLIENTE = B.ID_CLIENTE
			  AND A.ID_CLIENTE = C.ID_CLIENTE
</value>
  </data>
